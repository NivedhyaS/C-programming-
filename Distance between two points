#include<stdio.h>
#include<math.h>
int main()
{
	float distance,x1,x2,y1,y2;
	printf("enter coordinates of first point\n");
	scanf("%f%f",&x1,&y1);
	printf("enter coordinates of second point\n");
	scanf("%f%f",&x2,&y2);

	
	distance=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
	printf("distance:%.2f\n",distance);
	return 0;
}
