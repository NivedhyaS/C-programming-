#include<stdio.h>
int main()
{
	int a[20],i,n,even,odd;
	printf("enter the elements in the array:");
	scanf("%d",&n);
	printf("elements in the array:\n");
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	for(i=1;i<n;i+=2){
		printf("%d ",a[i]);
	}
	for(i=0;i<n;i+=2){
		printf("%d ",a[i]);
	}

	return 0;
}

