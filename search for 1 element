#include<stdio.h>
int main()
{
	int i,n,a[10],element;
	printf("enter the limit:");
	scanf("%d",&n);
	for(i=0;i<n;i++){
		printf("enter the elements in array:");
		scanf("%d",&a[i]);
	}
	printf("enter the element to search for:");
	scanf("%d",&element);
	int f1=0;
	for(i=0;i<n;i++){
		if (a[i]==element) f1=1;
	}	
	if(f1){
		printf("element found");
	}
	else {
		printf("element not found");
	}
	return 0;
}
